{
  "name": "substack-clone-api",
  "version": "1.0.0",
  "description": "api for substack-clone express ts, mtsql, docker",
  "main": "server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "prisma:generate": "prisma generate",
